<body style="margin: 0; padding: 0; width: 100%; height: 100%; overflow: hidden">
    <canvas id="canvas" style="background: silver"></canvas>
    <div id='path' style="overflow-y: visible;"></div>
    <input type="button" class="data_json" value="Save Data"></input>
    <input type="button" class="clear_canvas" value="Clear"></input>
<div id="mySidenav" class="sidenav">
  <a href="javascript:void(0)" class="closebtn">&times;</a>
  <% if user_signed_in? %>
      <li>
      <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
      </li>
    <% end %>
    <select name="users" id="users_all">
    <% @users.each do |user| %>
        <option value=<%= user[:id] %>><%= user[:email] %></option>
    <% end %>
    </select>
    <select name="gestures" id="gesture_all">
        <option value="0">Select Gesture</option>
      <% @gestures.each do |gesture| %>
        <option value=<%= gesture[:id] %>><%= gesture[:name] %></option>
    <% end %>
    </select>
    <input type="button" class="plot_ges" value="Plot Data">
</div>

<!-- Use any element to open the sidenav -->
<span class="open_navslide" >>></span>
</body>
